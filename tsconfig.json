{
    "extends": "./.dev/tsconfig.json",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "types": ["reflect-metadata"],
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "lib": [
            "DOM",
            "ES2017"
        ],
      "jsx": "react"
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "./src/**/*.ts",
        "./src/**/*.tsx"
    ]
}
